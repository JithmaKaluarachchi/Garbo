<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".user_onboarding_sc4">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/materialToolbaron4"
        android:layout_width="500dp"
        android:layout_height="16dp"
        android:background="#041F18"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/materialToolbaron5"
        android:layout_width="500dp"
        android:layout_height="16dp"
        android:background="#041F18"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView133"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/frijole"
        android:text="@string/sign_up"
        android:textAlignment="center"
        android:textColor="#041F18"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/textInputEditText24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.256" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText24"
        android:layout_width="366dp"
        android:layout_height="48dp"
        android:drawableStart="@drawable/baseline_person_24"
        android:fontFamily="@font/alegreya_sans_sc_bold"
        android:hint="@string/name"
        android:textColor="#041F18"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextPhone12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.896" />

    <EditText
        android:id="@+id/editTextPhone12"
        android:layout_width="371dp"
        android:layout_height="53dp"
        android:autofillHints=""
        android:drawableStart="@drawable/baseline_phone_android_24"
        android:ems="10"
        android:fontFamily="@font/alegreya_sans_sc_bold"
        android:hint="@string/mobile_number"
        android:inputType="phone"
        android:textColor="#041F18"
        android:textColorHint="#78909C"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPostalAddress12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.911" />

    <EditText
        android:id="@+id/editTextTextPostalAddress12"
        android:layout_width="371dp"
        android:layout_height="53dp"
        android:autofillHints=""
        android:drawableStart="@drawable/baseline_account_balance_24"
        android:ems="10"
        android:fontFamily="@font/alegreya_sans_sc_bold"
        android:hint="@string/home_address"
        android:inputType="textPostalAddress"
        android:textColor="#041F18"
        android:textColorHint="#78909C"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPostalAddress2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.945" />


    <EditText
        android:id="@+id/editTextTextPostalAddress2"
        android:layout_width="371dp"
        android:layout_height="53dp"
        android:layout_marginTop="336dp"
        android:autofillHints=""
        android:drawableStart="@drawable/baseline_account_balance_24"
        android:ems="10"
        android:fontFamily="@font/alegreya_sans_sc_bold"
        android:hint="@string/muncipal_council"
        android:inputType="textPostalAddress"
        android:textColor="#041F18"
        android:textColorHint="#78909C"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.819" />

    <EditText
        android:id="@+id/editTextTextPassword12"
        android:layout_width="371dp"
        android:layout_height="53dp"
        android:layout_marginBottom="8dp"
        android:autofillHints=""
        android:drawableStart="@drawable/baseline_adf_scanner_24"
        android:ems="10"
        android:fontFamily="@font/alegreya_sans_sc_bold"
        android:hint="@string/password"
        android:inputType="textPassword"
        android:textColor="#041F18"
        android:textColorHint="#78909C"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="371dp"
        android:layout_height="53dp"
        android:autofillHints=""
        android:drawableStart="@drawable/baseline_adf_scanner_24"
        android:ems="10"
        android:fontFamily="@font/alegreya_sans_sc_bold"
        android:hint="@string/confirm_password"
        android:inputType="textPassword"
        android:lineSpacingExtra="20sp"
        android:textColorHint="#78909C"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.641" />

    <CheckBox
        android:id="@+id/checkBox34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alegreya_sans_sc_bold"
        android:text="@string/rules"
        android:textColor="#041F18"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button44"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
        app:layout_constraintVertical_bias="0.117" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button44"
        android:layout_width="269dp"
        android:layout_height="48dp"
        android:background="#041F18"
        android:fontFamily="@font/alegreya_sans_sc_bold"
        android:text="@string/sign_in"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.862"
        tools:ignore="TextSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>